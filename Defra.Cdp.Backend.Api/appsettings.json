{
  "Mongo": {
    "DatabaseUri": "mongodb://cluster.address/admin?authSource=$external&authMechanism=MONGODB-AWS",
    "DatabaseName": "cdp-portal-backend"
  },
  "AllowedHosts": "*",
  "EcsEvents": {
    "QueueUrl": "http://127.0.0.1:4566/000000000000/ecs-deployments",
    "Enabled": true
  },
  "EcrEvents": {
    "QueueUrl": "http://127.0.0.1:4566/000000000000/ecr-push-events",
    "WaitTimeSeconds": 15,
    "Enabled": true
  },
  "EnvironmentMappings": {
    "555555555": "snd",
    "666666666": "management",
    "777777777": "infra-dev",
    "333333333": "development",
    "444444444": "test",
    "222222222": "perf-test",
    "111111111": "production"
  },
  "TrustStore": {
    "CertsToInstall": [
      "/app/rds-combined-ca-bundle.p7b"
    ]
  },
  "ContainerToIgnore": [
    "log_router",
    "cwagent",
    "aws-for-fluent-bit",
    "cloudwatch-agent"
  ],
  "Serilog": {
    "LogGroup": "cdp-portal-backend",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  }
}
