{
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "RegionName": "eu-west-2"
    },
    "Config": {
      "LocalStackHost": "localhost",
      "EdgePort": 4566
    }
  },
  "Mongo": {
    "DatabaseUri": "mongodb://127.0.0.1:27017?tls=false",
    "DatabaseName": "cdp-portal-backend"
  },
  "Github": {
    "PollIntervalSecs": 6000
  },
  "RepositoriesCreation": {
    "PollIntervalSecs": 20
  },
  "MigrationsBucket": "cdp-migrations",
  "SelfServiceOpsSecret": "9fb76f1abc39a08f3147fcdb5cb7ad9cb8dc3609af47c0553b81ac116109e070",
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "EcrEvents": {
    "WaitTimeSeconds": 15,
    "Enabled": true
  },
  "TrustStore": {
  },
  "EnvironmentMappings": {
    "111111111": "prod",
    "222222222": "perf-test",
    "333333333": "dev",
    "444444444": "test",
    "666666666": "management",
    "777777777": "infra-dev",
    "888888888": "ext-test"
  },
  "BucketSuffixes": {
    "prod": "75ee2",
    "perf-test": "05244",
    "dev": "c63f2",
    "test": "6bf3a",
    "management": "8dfff",
    "infra-dev":  "7df0c",
    "ext-test": "8ec5c"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore.Hosting": "Warning",
        "Microsoft.AspNetCore.Http": "Warning",
        "System": "Warning",
        "Quartz": "Warning",
        "Defra.Cdp.Backend.Api.Mongo": "Information",
        "Defra.Cdp.Backend.Api.Mongo.MongoLock": "Warning",
        "Defra.Cdp.Backend.Api.Services.Github": "Warning",
        "Defra.Cdp.Backend.Api": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}